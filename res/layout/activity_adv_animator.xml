<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/background"
    tools:context=".AdvAnimatorActivity" >
    <TableLayout
            android:id="@+id/container1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TableRow
                android:id="@+id/container2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="20dp">

            <ImageButton
                    android:id="@+id/btnimage"
                    android:src="@drawable/add_devices"
                    android:layout_width="110dp"
                    android:layout_height="110dp"
                    android:background="@null"
                    android:gravity="center"
                    android:scaleType="centerCrop"
                    />
            <ViewFlipper
                    android:id="@+id/viewFlipperNew"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew1"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew2"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:scaleType="centerCrop"
                        android:layout_gravity="center"
                        android:clickable="true"/>
            </ViewFlipper>
            <ViewFlipper
                    android:id="@+id/viewFlipperNew1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew3"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew4"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:scaleType="centerCrop"
                        android:layout_gravity="center"
                        android:clickable="true"/>
            </ViewFlipper>
        </TableRow>
        <TableRow
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
            <TextView
                    android:id="@+id/textbtn"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:text="Add device"
                    android:layout_gravity="center"/>
            <TextView
                    android:id="@+id/textView1"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
            <TextView
                    android:id="@+id/textView2"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
        </TableRow>
        <TableRow
                android:id="@+id/container3"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="20dp">
            <ViewFlipper
                    android:id="@+id/viewFlipperNew2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew5"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew6"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:scaleType="centerCrop"
                        android:layout_gravity="center"
                        android:clickable="true"/>
            </ViewFlipper>
            <ViewFlipper
                    android:id="@+id/viewFlipperNew3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew7"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew8"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>
            </ViewFlipper>
            <ViewFlipper
                    android:id="@+id/viewFlipperNew4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew9"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:background="@null"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew10"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>
            </ViewFlipper>

        </TableRow>
        <TableRow
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
            <TextView
                    android:id="@+id/textView3"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
            <TextView
                    android:id="@+id/textView4"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
            <TextView
                    android:id="@+id/textView5"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
        </TableRow>
        <TableRow
                android:id="@+id/container4"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="20dp">
            <ViewFlipper
                    android:id="@+id/viewFlipperNew5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew11"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew12"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:scaleType="centerCrop"
                        android:layout_gravity="center"
                        android:clickable="true"/>
            </ViewFlipper>
            <ViewFlipper
                    android:id="@+id/viewFlipperNew6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">
                <ImageView
                        android:id="@+id/imageViewNew13"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>

                <ImageView
                        android:id="@+id/imageViewNew14"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:clickable="true"/>
            </ViewFlipper>


        </TableRow>
        <TableRow
                android:layout_height="wrap_content"
                android:layout_width="wrap_content">
            <TextView
                    android:id="@+id/textView6"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>
            <TextView
                    android:id="@+id/textView7"
                    android:layout_height="wrap_content"
                    android:layout_width="110dp"
                    android:layout_gravity="center"/>

        </TableRow>
    </TableLayout>

</LinearLayout>
